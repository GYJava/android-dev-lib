            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text="TDS:100"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/power"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text="电量:100%"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/DetectTime1"
            android:checked="false"
            android:text="12:50"
            />
        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/DetectTime2"
            android:checked="false"

            android:text="12:50"
            />
        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/DetectTime3"
            android:checked="false"

            android:text="12:50"
            />
        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/DetectTime4"
            android:checked="false"

            android:text="12:50"
            />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/save"
            android:text="保存"
            />
    </LinearLayout>
    <ListView
        android:id="@+id/tdsList"
        android:layout_width="match_parent"
        android:layout_height="150dp" />
</ozner.xzy.test.TapItemView>